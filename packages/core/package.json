    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vueuse/core": "^11.2.0",
    "commander": "^12.1.0",
    "defu": "^6.1.4",
    "highlight.js": "^11.10.0",
    "importx": "^0.5.0",
    "js-yaml": "^4.1.0",
