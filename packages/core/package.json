    "unplugin-vue": "^5.1.5",
    "vite": "^5.4.11",
    "vite-plugin-inspect": "^0.8.7",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "zod": "^3.23.8"
