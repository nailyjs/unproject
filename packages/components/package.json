{
  "name": "@unproject/components",
  "type": "module",
  "version": "2.0.5",
  "description": "Components for unproject.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-swc",
    "watch": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-swc --watch --watch.exclude=./dist/**/*",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@iconify-json/ph": "^1.2.1",
    "@nailyjs/ioc": "^2.0.6",
    "@nailyjs/zod": "^2.0.8",
    "@unproject/core": "workspace:*",
    "@vueuse/core": "^11.3.0",
    "naive-ui": "^2.40.1",
    "reflect-metadata": "^0.2.2",
    "unocss": "^0.64.1",
    "unplugin-vue": "^5.1.5",
    "vue": "^3.5.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-swc": "^0.4.0",
    "postcss": "^8.4.49",
    "rollup": "^4.27.3",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "tsup": "^8.3.0",
    "zod": "^3.23.8"
  }
}
