    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "commander": "^12.1.0",
    "defu": "^6.1.4",
    "load-tsconfig": "^0.2.5",
    "naive-ui": "^2.40.1",
    "unplugin": "^1.16.0",
    "vite": "^5.4.11",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "tsup": "^8.3.0"
  }
}
