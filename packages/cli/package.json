{
  "name": "@nailyjs/uncli",
  "type": "module",
  "version": "0.0.1",
  "description": "A cli tool for configure project.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
